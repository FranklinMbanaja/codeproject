class=3Dcode-keyword>false</SPAN>;
        }
<SPAN class=3Dcode-preprocessor>#if</SPAN> defined(__ATLWINCE_H__)
        SetFocus();
<SPAN class=3Dcode-preprocessor>#endif</SPAN>
        <SPAN class=3Dcode-keyword>return</SPAN> <SPAN =
class=3Dcode-keyword>true</SPAN>;
}</PRE>
